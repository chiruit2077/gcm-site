<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:c="urn:import:br.com.ecc.client.ui.component"
  >
  	<ui:style>
		.fundo {
			width: 100%;
			padding: 0px;
		}
		.menu {
			padding: 2px;
			border: 1px solid white;
			cursor: pointer;
		}
		.menu:hover {
			border: 1px solid lightgray;
			background-color: #f3f3f3;
			color: blue;
		}
		.portafolio {
			padding: 5px;

		}
		.negrito {
			padding-left: 5px;
		}
		.produtos {
			font-size: 12px;
		}
	</ui:style>
	<g:HorizontalPanel ui:field="homePanel" styleName="{style.fundo}" horizontalAlignment="center">
		<g:HTMLPanel>
			<table cellpading="0" cellspacing="0" border="0" align="center">
				<tr><td>&nbsp;</td></tr>
				<tr><td>&nbsp;</td></tr>
				<tr>
					<td align="center">
						<!-- <g:Image url="images/ecc.png"/> -->
						<g:HorizontalPanel ui:field="gruposPanel" width="100%">
						</g:HorizontalPanel>
					</td>
				</tr>
				<tr><td>&nbsp;</td></tr>
				<tr>
					<td align="center">
						<g:Anchor ui:field="loginAnchor" text="Entrar ao sistema"/>
					</td>
				</tr>
			</table>
		</g:HTMLPanel>
	</g:HorizontalPanel>
 </ui:UiBinder>